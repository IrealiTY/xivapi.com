#!/usr/bin/env bash

START_TIME=$SECONDS

## -------------------------------------------------------------------------------------

echo "Logging into account A"
/usr/bin/php /vagrant/bin/console CompanionAppLoginCommand A

sleep 5

echo "Logging into account B"
/usr/bin/php /vagrant/bin/console CompanionAppLoginCommand B

## -------------------------------------------------------------------------------------

end=`date +%s`

ELAPSED_TIME=$(($SECONDS - $START_TIME))

echo ""
echo "Completed"
echo "$(($ELAPSED_TIME/60)) min $(($ELAPSED_TIME%60)) sec"
echo ""
