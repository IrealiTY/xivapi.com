{% extends "base.html.twig"  %}
{% block title %}Statistics{% endblock %}

{% block body %}

    <div class="two-pane">

        <aside>
            <h3>Queue</h3>

            <span>
                <p>Rabbit MQ Stats</p>
            </span>

        </aside>

        <main class="stats">
            <h1>24hour Period Statistics</h1>

            <div class="container">
                <div class="row">
                    <div class="col-sm">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title">Average Duration</h5>
                                <p class="card-text">{{ stats.average_duration }}</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title">Counts</h5>
                                <p class="card-text">
                                    {{ stats.counts.min }} past minute<br>
                                    {{ stats.counts.hr }} past hour<br>
                                    {{ stats.counts.day }} past day
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <hr>

            <h2>Characters</h2>

            <div class="container">
                <div class="row">
                    <div class="col-sm">
                        <ul class="list-group">
                            <li class="list-group-item">{{ stats.queue_stats.character_add ?: 0 }} character_add</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update ?: 0 }} character_update</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_0_normal ?: 0 }} character_update_0_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_1_normal ?: 0 }} character_update_1_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_2_normal ?: 0 }} character_update_2_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_3_normal ?: 0 }} character_update_3_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_4_normal ?: 0 }} character_update_4_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_5_normal ?: 0 }} character_update_5_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_0_patreon ?: 0 }} character_update_0_patreon</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_1_patreon ?: 0 }} character_update_1_patreon</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_0_low ?: 0 }} character_update_0_low</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_update_1_low ?: 0 }} character_update_1_low</li>
                        </ul>
                    </div>
                    <div class="col-sm">
                        <ul class="list-group">
                            <li class="list-group-item">{{ stats.queue_stats.character_friends_add ?: 0 }} character_friends_add</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_friends_update ?: 0 }} character_friends_update</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_friends_update_0_normal ?: 0 }} character_friends_update_0_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_friends_update_1_normal ?: 0 }} character_friends_update_1_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_friends_update_0_patreon ?: 0 }} character_friends_update_0_patreon</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_friends_update_1_patreon ?: 0 }} character_friends_update_1_patreon</li>
                        </ul>
                    </div>
                    <div class="col-sm">
                        <ul class="list-group">
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_add ?: 0 }} character_achievements_add</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update ?: 0 }} character_achievements_update</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_0_normal ?: 0 }} character_achievements_update_0_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_1_normal ?: 0 }} character_achievements_update_1_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_2_normal ?: 0 }} character_achievements_update_2_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_3_normal ?: 0 }} character_achievements_update_3_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_4_normal ?: 0 }} character_achievements_update_4_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_5_normal ?: 0 }} character_achievements_update_5_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_0_patreon ?: 0 }} character_achievements_update_0_patreon</li>
                            <li class="list-group-item">{{ stats.queue_stats.character_achievements_update_1_patreon ?: 0 }} character_achievements_update_1_patreon</li>
                        </ul>
                    </div>
                </div>
            </div>

            <h2>FC, LS, PVP</h2>

            <div class="container">
                <div class="row">
                    <div class="col-sm">
                        <ul class="list-group">
                            <li class="list-group-item">{{ stats.queue_stats.free_company_add ?: 0 }} free_company_add</li>
                            <li class="list-group-item">{{ stats.queue_stats.free_company_update ?: 0 }} free_company_update</li>
                            <li class="list-group-item">{{ stats.queue_stats.free_company_update_0_normal ?: 0 }} free_company_update_0_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.free_company_update_1_normal ?: 0 }} free_company_update_1_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.free_company_update_0_patron ?: 0 }} free_company_update_0_patron</li>
                            <li class="list-group-item">{{ stats.queue_stats.free_company_update_1_patron ?: 0 }} free_company_update_1_patron</li>
                        </ul>
                    </div>
                    <div class="col-sm">
                        <ul class="list-group">
                            <li class="list-group-item">{{ stats.queue_stats.linkshell_add ?: 0 }} linkshell_add</li>
                            <li class="list-group-item">{{ stats.queue_stats.linkshell_update ?: 0 }} linkshell_update</li>
                            <li class="list-group-item">{{ stats.queue_stats.linkshell_update_0_normal ?: 0 }} linkshell_update_0_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.linkshell_update_1_normal ?: 0 }} linkshell_update_1_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.linkshell_update_0_patron ?: 0 }} linkshell_update_0_patron</li>
                            <li class="list-group-item">{{ stats.queue_stats.linkshell_update_1_patron ?: 0 }} linkshell_update_1_patron</li>
                        </ul>
                    </div>
                    <div class="col-sm">
                        <ul class="list-group">
                            <li class="list-group-item">{{ stats.queue_stats.pvp_team_add ?: 0 }} pvp_team_add</li>
                            <li class="list-group-item">{{ stats.queue_stats.pvp_team_update ?: 0 }} pvp_team_update</li>
                            <li class="list-group-item">{{ stats.queue_stats.pvp_team_update_0_normal ?: 0 }} pvp_team_update_0_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.pvp_team_update_1_normal ?: 0 }} pvp_team_update_1_normal</li>
                            <li class="list-group-item">{{ stats.queue_stats.pvp_team_update_0_patron ?: 0 }} pvp_team_update_0_patron</li>
                            <li class="list-group-item">{{ stats.queue_stats.pvp_team_update_1_patron ?: 0 }} pvp_team_update_1_patron</li>
                        </ul>
                    </div>
                </div>
            </div>

            <hr>

            {{ dump(stats) }}
        </main>

    </div>

{% endblock %}
